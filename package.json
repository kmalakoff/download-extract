{
  "name": "get-remote",
  "version": "1.3.0",
  "description": "Download a text, json, a file with optional extract, get a stream, or head an endpoint",
  "keywords": [
    "download",
    "file",
    "extract"
  ],
  "homepage": "https://github.com/kmalakoff/get-remote",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/get-remote.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "content-disposition": "^0.5.4",
    "end-of-stream": "^1.4.4",
    "function-exec-sync": "^1.1.2",
    "lazy-cache": "^2.0.2",
    "mkdirp-classic": "^0.5.3",
    "node-exec-path": "^1.0.0",
    "object-assign": "^4.1.1",
    "os-shim": "^0.1.3",
    "pinkie-promise": "^2.0.1",
    "progress-stream": "^2.0.0",
    "pump": "^3.0.2",
    "readable-stream": "^2.3.7",
    "require_optional": "^1.0.1",
    "rimraf2": "^2.8.2",
    "temp-suffix": "^0.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fast-extract": "^1.3.0",
    "fs-iterator": "^5.1.1",
    "fs-stats-spys": "^1.1.0",
    "is-tar": "^1.0.0",
    "lzma-native": "^8.0.6",
    "supports-color": "^3.2.3",
    "ts-dev-stack": "^1.5.4"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
