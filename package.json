{
  "name": "get-remote",
  "version": "0.8.4",
  "description": "Download a text, json, a file with optional extract, get a stream, or head an endpoint",
  "keywords": [
    "download",
    "file",
    "extract"
  ],
  "homepage": "https://github.com/kmalakoff/get-remote",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/get-remote.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node --timeout=10000",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "content-disposition": "^0.5.4",
    "core-js": "^3.36.0",
    "end-of-stream": "^1.4.4",
    "function-exec-sync": "^0.2.11",
    "mkdirp": "^0.5.6",
    "mkpath": "^1.0.0",
    "node-exec-path": "^0.1.0",
    "os-shim": "^0.1.3",
    "progress-stream": "^2.0.0",
    "pump": "^3.0.0",
    "readable-stream": "^2.3.7",
    "require_optional": "^1.0.1",
    "rimraf": "^2.7.1",
    "temp-suffix": "^0.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fast-extract": "^1.0.1",
    "fs-iterator": "^5.0.1",
    "fs-stats-spys": "^1.0.2",
    "is-tar": "^1.0.0",
    "lzma-native": "^8.0.6",
    "supports-color": "^3.2.3",
    "ts-dev-stack": "^0.14.9"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
